{
  "name": "dokki",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@types/jest": "24.0.23",
    "@types/node": "12.12.14",
    "@types/react": "^16.9.13",
    "@types/react-dom": "16.9.4",
    "axios": "0.19.0",
    "bootstrap": "4.4.1",
    "connected-react-router": "6.5.2",
    "faker": "4.1.0",
    "formik": "2.0.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-icons": "3.8.0",
    "react-intl": "3.3.0",
    "react-loadable": "^5.5.0",
    "react-redux": "7.1.1",
    "react-router-dom": "5.1.2",
    "react-scripts": "file:./react-scripts",
    "reactstrap": "8.2.0",
    "redux": "4.0.1",
    "redux-devtools-extension": "2.13.8",
    "redux-persist": "6.0.0",
    "redux-saga": "1.1.1",
    "reselect": "4.0.0",
    "shared-reducers-toddo": "0.2.0",
    "typescript": "3.7.4"
  },
  "scripts": {
    "start": "serve -s build",
    "build": "cross-env GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test --watchAll=false --collectCoverage=true --coverageReporters=html",
    "eject": "react-scripts eject",
    "dev": "react-scripts dev",
    "build:sourcemap": "react-scripts build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "disabled-postbuild": "purgecss --css build/static/css/*.css --content build/static/index.html build/static/js/*.js --out build/static/css",
    "heroku-postbuild": "npm run build",
    "test:watch": "react-scripts test --collectCoverage=true --coverageReporters=text",
    "test:e2e": "jest -c integration/jest.config.js --collectCoverage=true --coverageReporters=html",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "eslint": "eslint src/ --ext .ts --ext .tsx",
    "eslint:fix": "eslint src/ --ext .ts --ext .tsx --fix"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "jest": {
    "moduleNameMapper": {
      "~types/(.*)": "<rootDir>/src/types/$1",
      "~fonts/(.*)": "<rootDir>/src/assets/fonts/$1",
      "~images/(.*)": "<rootDir>/src/assets/images/$1",
      "~styles/(.*)": "<rootDir>/src/assets/styles/$1",
      "~components/(.*)": "<rootDir>/src/components/$1",
      "~constants/(.*)": "<rootDir>/src/constants/$1",
      "~lang/(.*)": "<rootDir>/src/lang/$1",
      "~navigation/(.*)": "<rootDir>/src/navigation/$1",
      "~actions/(.*)": "<rootDir>/src/redux/actions/$1",
      "~reducers/(.*)": "<rootDir>/src/redux/reducers/$1",
      "~sagas/(.*)": "<rootDir>/src/redux/sagas/$1",
      "~selectors/(.*)": "<rootDir>/src/redux/selectors/$1",
      "~screens/(.*)": "<rootDir>/src/screens/$1",
      "~services/(.*)": "<rootDir>/src/services/$1",
      "~utils/(.*)": "<rootDir>/src/utils/$1"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "1.3.0",
    "@types/enzyme": "3.10.4",
    "@types/enzyme-adapter-react-16": "1.0.5",
    "@types/expect-puppeteer": "3.3.3",
    "@types/faker": "4.1.7",
    "@types/jest-environment-puppeteer": "4.3.1",
    "@types/lodash": "4.14.149",
    "@types/puppeteer": "2.0.0",
    "@types/react-loadable": "5.5.2",
    "@types/react-redux": "7.1.5",
    "@types/react-router": "5.1.3",
    "@types/react-router-dom": "5.1.3",
    "@types/reactstrap": "8.2.0",
    "@typescript-eslint/eslint-plugin": "2.7.0",
    "babel-eslint": "10.0.3",
    "cross-env": "6.0.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.1",
    "eslint": "6.6.0",
    "eslint-config-prettier": "6.5.0",
    "eslint-config-react-native": "4.0.0",
    "eslint-config-semistandard": "15.0.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.17.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-native": "3.7.0",
    "eslint-plugin-standard": "4.0.1",
    "husky": "3.1.0",
    "jest-enzyme": "7.1.2",
    "jest-puppeteer": "4.3.0",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1",
    "prettier-eslint": "9.0.1",
    "puppeteer": "2.0.0",
    "serve": "11.1.0",
    "source-map-explorer": "2.0.1",
    "ts-jest": "24.2.0"
  },
  "bit": {
    "env": {},
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  }
}
